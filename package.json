{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "raycast-extension-template-collection",
  "title": "Raycast Extension Template Collection",
  "description": "Raycast Extension Template Collection Description",
  "icon": "extension-icon.png",
  "author": "rmncldyo",
  "license": "MIT",
  "commands": [
    {
      "name": "show-detail",
      "title": "Show Detail",
      "subtitle": "Raycast Extension Template Collection",
      "description": "Show Detail Command Description",
      "mode": "view"
    },
    {
      "name": "submit-form",
      "title": "Submit Form",
      "subtitle": "Raycast Extension Template Collection",
      "description": "Submit Form Command Description",
      "mode": "view"
    },
    {
      "name": "show-grid",
      "title": "Show Grid",
      "subtitle": "Raycast Extension Template Collection",
      "description": "Show Grid Command Description",
      "mode": "view"
    },
    {
      "name": "show-list-and-detail",
      "title": "Show List and Detail",
      "subtitle": "Raycast Extension Template Collection",
      "description": "Show List and Detail Command Description",
      "mode": "view"
    },
    {
      "name": "menu-bar-extra",
      "title": "Menu Bar Extra",
      "subtitle": "Raycast Extension Template Collection",
      "description": "Menu Bar Extra Command Description",
      "mode": "menu-bar"
    },
    {
      "name": "run-script",
      "title": "Run Script",
      "subtitle": "Raycast Extension Template Collection",
      "description": "Run Script Command Description",
      "mode": "no-view"
    },
    {
      "name": "show-list",
      "title": "Show List",
      "subtitle": "Raycast Extension Template Collection",
      "description": "Show List Command Description",
      "mode": "view"
    },
    {
      "name": "show-typehead-results",
      "title": "Show Typehead Results",
      "subtitle": "Raycast Extension Template Collection",
      "description": "Show Typehead Results Command Description",
      "mode": "view"
    },
    {
      "name": "ai",
      "title": "AI",
      "subtitle": "Raycast Extension Template Collection",
      "description": "AI Command Description",
      "mode": "view"
    },
    {
      "name": "blank",
      "title": "Blank",
      "subtitle": "Raycast Extension Template Collection",
      "description": "Blank Command Description",
      "mode": "no-view"
    }
  ],
  "tools": [
    {
      "name": "blank-tool",
      "title": "Blank Tool",
      "description": "Blank Tool Description"
    },
    {
      "name": "tool-with-confirmation",
      "title": "Tool with Confirmation",
      "description": "Tool with Confirmation Description"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.99.3",
    "@raycast/utils": "^1.19.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^2.0.4",
    "@types/node": "22.15.26",
    "@types/react": "19.1.6",
    "eslint": "^9.27.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3"
  },
  "scripts": {
    "build": "ray build",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "prepublishOnly": "echo \"\\n\\nIt seems like you are trying to publish the Raycast extension to npm.\\n\\nIf you did intend to publish it to npm, remove the \\`prepublishOnly\\` script and rerun \\`npm publish\\` again.\\nIf you wanted to publish it to the Raycast Store instead, use \\`npm run publish\\` instead.\\n\\n\" && exit 1",
    "publish": "npx @raycast/api@latest publish"
  }
}