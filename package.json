{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "raycast-extension-templates",
  "title": "Raycast Extension Templates",
  "description": "A collection of Raycast extension templates.",
  "icon": "extension-icon.png",
  "author": "rmncldyo",
  "license": "MIT",
  "commands": [
    {
      "name": "blank-command",
      "title": "Blank Command Title",
      "subtitle": "Black command subtitle",
      "description": "Blank command description.",
      "mode": "no-view"
    },
    {
      "name": "show-detail",
      "title": "Show Detail Title",
      "subtitle": "Show detail subtitle",
      "description": "Show detail description.",
      "mode": "view"
    },
    {
      "name": "submit-form",
      "title": "Submit Form Title",
      "subtitle": "Submit form subtitle",
      "description": "Submit form description.",
      "mode": "view"
    },
    {
      "name": "show-grid",
      "title": "Show Grid Title",
      "subtitle": "Show grid subtitle",
      "description": "Show grid description.",
      "mode": "view"
    },
    {
      "name": "show-list-and-detail",
      "title": "Show List And Detail Title",
      "subtitle": "Show list and detail subtitle",
      "description": "Show list and detail description.",
      "mode": "view"
    },
    {
      "name": "menu-bar-extra",
      "title": "Menu Bar Extra Title",
      "subtitle": "Menu bar extra subtitle",
      "description": "Menu bar extra description.",
      "mode": "menu-bar"
    },
    {
      "name": "run-script",
      "title": "Run Script Title",
      "subtitle": "Run script subtitle",
      "description": "Run script description.",
      "mode": "no-view"
    },
    {
      "name": "show-list",
      "title": "Show List Title",
      "subtitle": "Show list subtitle",
      "description": "Show list description.",
      "mode": "view"
    },
    {
      "name": "show-typeahead-results",
      "title": "Show Typeahead Results Title",
      "subtitle": "Show typeahead results subtitle",
      "description": "Show typeahead results description.",
      "mode": "view"
    },
    {
      "name": "ai",
      "title": "AI Title",
      "subtitle": "AI subtitle",
      "description": "AI description.",
      "mode": "view"
    }
  ],
  "tools": [
    {
      "name": "blank-tool",
      "title": "Blank Tool Title",
      "description": "Blank tool description."
    },
    {
      "name": "tool-with-confirmation",
      "title": "Tool With Confirmation Title",
      "description": "Tool with confirmation description."
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.99.4",
    "@raycast/utils": "^1.17.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^2.0.4",
    "@types/node": "22.13.10",
    "@types/react": "19.0.10",
    "eslint": "^9.22.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2"
  },
  "scripts": {
    "build": "ray build",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "prepublishOnly": "echo \"\\n\\nIt seems like you are trying to publish the Raycast extension to npm.\\n\\nIf you did intend to publish it to npm, remove the \\`prepublishOnly\\` script and rerun \\`npm publish\\` again.\\nIf you wanted to publish it to the Raycast Store instead, use \\`npm run publish\\` instead.\\n\\n\" && exit 1",
    "publish": "npx @raycast/api@latest publish"
  }
}